<% if (success && success.length) { %>
<div
  class="alert mt-3 mx-auto col-6 alert-success alert-dismissible fade show d-flex justify-content-center align-items-center position-relative"
  role="alert"
  style="height: 50px"
>
  <h4 class="m-0"><%= success %></h4>
  <button
    type="button"
    class="btn-close position-absolute end-0 me-3"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>
<% } %> 

